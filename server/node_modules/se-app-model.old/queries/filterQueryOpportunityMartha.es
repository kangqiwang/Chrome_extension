GET /opportunity-martha-us/_search
{
  "query": {
    "bool": {
      "must": [
        {
          "range": {
            "amazon.price": {
              "gte": 1
            }
          }
        },
        {
          "range": {
            "amazon.details.profit": {
              "gte": 0
            }
          }
        },
        {
          "range": {
            "amazon.details.ROI": {
              "gte": 0
            }
          }
        },
        {
          "range": {
            "amazon.details.salesRankPercentage": {
              "lte": 44,
              "gt": -1
            }
          }
        }
      ],
      "must_not": []
    }
  },
  "aggs": {
    "suppliers": {
      "terms": {
        "field": "supolier.domain",
        "size": 100
      }
    },
    "categories": {
      "terms": {
        "field": "amazon.category",
        "size": 100
      }
    },
    "brandRestrictions": {
      "terms": {
        "field": "amazon.brandRestrictionResults.resultCode"
      }
    }
  },
  "sort": [
    {
      "created": {
        "order": "desc"
      }
    }
  ]
}
